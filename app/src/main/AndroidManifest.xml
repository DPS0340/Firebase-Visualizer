<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="xyz.dps0340.firebase_visualizer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/firebase_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name="xyz.dps0340.firebase_visualizer.ViewerActivity"
            android:label="@string/title_activity_viewer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="xyz.dps0340.firebase_visualizer.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>